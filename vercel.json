{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install && npm install --prefix frontend",
    "outputDirectory": "frontend/build",
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/index.js"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  